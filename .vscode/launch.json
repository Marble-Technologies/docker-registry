    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Debug ECR Registry",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/registry/main.go",
            "args": [
                "serve",
                "${workspaceFolder}/cmd/registry/config-ecr.yml"
            ],
            "env": {
                "OTEL_TRACES_EXPORTER": "none"
            }
        }
    ]
}